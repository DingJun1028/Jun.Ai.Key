{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "preserve",
    "module": "commonjs",
    "baseUrl": "./src",
    "paths": {
      "*": ["*"],
      "@/*": ["*"],
      "agents/*": ["agents/*"],
      "services/*": ["services/*"],
      "models/*": ["models/*"],
      "components/*": ["components/*"],
      "hooks/*": ["hooks/*"]
    },
    "esModuleInterop": true,
    "sourceMap": true,
    "inlineSourceMap": false,
    "outDir": "dist",
    "typeRoots": [
      "./dts",
      "./node_modules/@types"
    ],
    "types": [
      "scripting"
    ]
  }
}
